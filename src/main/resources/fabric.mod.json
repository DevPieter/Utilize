{
  "schemaVersion": 1,
  "id": "utilize",
  "version": "${version}",
  "name": "Utilize",
  "description": "Yet another Minecraft modding library, made for DevPieter's mods.",
  "authors": [
    "DevPieter"
  ],
  "contact": {
    "homepage": "https://pietr.space/",
    "sources": "https://github.com/DevPieter/Utilize",
    "issues": "https://github.com/DevPieter/Utilize/issues"
  },
  "license": "MIT",
  "icon": "assets/utilize/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "nl.devpieter.utilize.Utilize"
    ]
  },
  "mixins": [
    "utilize.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version} <=${max_minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ]
    }
  }
}
